                            <div class="brand bg-dark opacity">
                                <span class="text">ПОЛИРОВКА С ПОНТАМИ</span>
                            </div>
                        </a>
                        <a href="http://www.topgearrussia.ru/columns/richard-hammond/227001/d0ef9147.html" class="tile-content image">
                            <img src="http://s08.topgearrussia.ru/sh/b648dcdc595c869c56d86ce22a51ee60/images/preview/f8/7b/8a/58e69ecc872d6a8d87b1d33bfe.jpeg">
                            <div class="brand bg-dark opacity">
                                <span class="text">ХАММОНД ЗА РУЛЕМ ЛЕГЕНДЫ: MITSUBISHI EVO VI</span>
                            </div>
                        </a>
                        <a href="http://www.topgearrussia.ru/tv-show/sguide/211038/6b21b924.html" class="tile-content image">
                            <img src="http://s09.topgearrussia.ru/sh/cc907e7512d603f77b9ed5b1d73cceb3/images/preview/21/a3/3f/8cc57d740a0ee3dd10c870e487.jpeg">
                            <div class="brand bg-dark opacity">
                                <span class="text">АМЕРИКАНСКОЕ ПУТЕШЕСТВИЕ НА LEXUS LFA, ASTON MARTIN VANQUISH И SRT VIPER - ЧАСТЬ 3</span>
                            </div>
                        </a>                                                
                    </div>
                </td>
            </tr>
        </table>        
    </div>

    <div id="workspace-list">
        <div class="listview-outlook" id="entities-list"></div>
        <div id="entity-details"></div>
    </div>

    <div id="workspace-empty">
        <div class="listview-outlook" id="entities-list"></div>
        <div id="entity-details"></div>
    </div>
        
</div>

<!--# ITEMS TEMPLATES #-->
@Html.Partial("_EngineItem")
@Html.Partial("_EngineItemInfo")
@Html.Partial("_EngineItemForm")
@Html.Partial("_JobTitleItem")
@Html.Partial("_JobTitleItemInfo")
@Html.Partial("_JobTitleItemForm")
@Html.Partial("_UserItem")
